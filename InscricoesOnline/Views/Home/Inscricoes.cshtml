@model InscricoesOnline.Models.Equipe

@{
    ViewBag.Title = "Inscricoes";
}

<div class="container">
    <div class="row evento-detalhe">
        @Model.Nome
        @Model.

        <hr />
        <div class="row">
            <div class="form-group col-md-2">
                <label for="txtCodFili">Código:</label>
                <input type="text" class="form-control txtCodFili" placeholder="Código" value="" data-toggle="tooltip" title="Digite o código do filiado e pressione Enter">
            </div>

            <div class="form-group col-md-4">
                <label>Nome do filiado:</label>
                <input type="text" class="form-control txtNome">
            </div>

            <div class="form-group col-md-3">
                <label>Faixa:</label>
                <input type="text" class="form-control txtFaixa">
            </div>

            <div class="form-group col-md-3">
                <label>Sexo:</label>
                <input type="text" class="form-control txtSexo">
            </div>
        </div>
    </div>
</div>